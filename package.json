{
  "name": "hr-admin-ui",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "20.x"
  },
  "packageManager": "npm@10.8.3",
  "scripts": {
    "build:css": "tailwindcss -i backend/apps/admin_ui/static/css/main.css -o backend/apps/admin_ui/static/build/main.css --minify",
    "lint:deps": "depcheck --config depcheck.config.cjs",
    "lint:cssjs": "node tools/static_lint.mjs"
  },
  "devDependencies": {
    "@tailwindcss/forms": "0.5.7",
    "@tailwindcss/typography": "0.5.12",
    "autoprefixer": "10.4.20",
    "depcheck": "1.4.7",
    "postcss": "8.4.41",
    "tailwindcss": "3.4.13"
  },
  "depcheck": {
    "ignoreMatches": [
      "autoprefixer",
      "postcss",
      "depcheck"
    ]
  }
}
